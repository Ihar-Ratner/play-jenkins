pipeline {
    agent any

    // triggers {
    //     githubPush()
    // }

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('PR') {
            steps {
                sh 'env'
            }
        }

    }

    post {
        always {
            echo 'Clean Workspace after build'
            cleanWs()
        }
    }
}
